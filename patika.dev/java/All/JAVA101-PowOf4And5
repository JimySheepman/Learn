import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int i = 1;
        int j = 1;
        int n;
        int m;

        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a number for pow of 4: ");
        n = scanner.nextInt();

        for (i = 1; i < n; i *= 4) {
            System.out.println("Pow of 4: " + i);
        }

        System.out.print("Enter a number for pow of 5: ");
        m = scanner.nextInt();

        for (j = 1; j < m; j *= 5) {
            System.out.println("Pow of 5: " + j);
        }
    }
}
