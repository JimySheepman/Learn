import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        int n;
        int k;
        int i = 1;
        int sum = 1;
        Scanner scanner = new Scanner(System.in);

        System.out.print("Number to base: ");
        n = scanner.nextInt();

        System.out.print("Number to be exponent: ");
        k = scanner.nextInt();

        for (i = 1; i <= k; i++) {
            sum *= n;
        }
        System.out.println(n + " pow " + k + " = " + sum);
    }
}
