<Window
    x:Class="lab_assingnment.Window2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:lab_assingnment"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Be Healthy Web"
    Width="800"
    Height="450"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>
        <DockPanel
            Width="795"
            Height="50"
            HorizontalAlignment="Left"
            VerticalAlignment="Top">
            <Button
                x:Name="show_page_button"
                Width="200"
                Height="40"
                Margin="0,0,0,0"
                Click="show_page_button_Click"
                Content="Show Web Page" />
            <Button
                x:Name="back_button"
                Width="200"
                Height="40"
                Margin="0,0,380,0"
                Click="back_button_Click"
                Content="Back To Menu" />
        </DockPanel>
        <Frame x:Name="web_frame" Margin="0,50,0,0" />
    </Grid>
</Window>
